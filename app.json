{
  "expo": {
    "name": "Liloba Vaulter",
    "slug": "liloba-vaulter",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/android/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/android/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#203554"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "allowBackup": false,
      "adaptiveIcon": {
        "backgroundColor": "#203554"
      },
      "package": "com.iamdamba.lilobavaulter"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/android/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-build-properties",
      "expo-font",
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "04ab250d-6d23-49c6-b5e3-b291a4bb410b"
      }
    }
  }
}
